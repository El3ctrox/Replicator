{
  "name": "Replicator",
  "tree": {
    "$className": "DataModel",
    
    "ReplicatedStorage": {
      "Packages": { "$path": "Packages" }
    },
    "ServerScriptService": {
      "Replicator": { "$path": "src/server" }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Replication": { "$path": "src/client" }
      }
    },
    
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}